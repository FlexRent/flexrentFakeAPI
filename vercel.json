{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "http://15.228.10.222",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    }
  ]
}
